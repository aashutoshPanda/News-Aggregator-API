  "bugs": {
    "url": "https://github.com/georgesimos/nodejs-mvc-starter/issues"
  },
  "homepage": "https://github.com/georgesimos/nodejs-mvc-starter#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.5",
    "morgan": "^1.10.0",
    "redis": "^4.6.6"
  },
  "devDependencies": {
    "chai": "^4.3.7",
