{
  "name": "nodejs-mvc-starter",
  "version": "1.0.0",
  "description": "Nodejs Starter - A boilerplate for Node.js mvc web applications",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "debug": "nodemon --inspect index.js",
    "redis-start": "sudo systemctl start redis",
    "redis-status": "sudo systemctl status redis",
    "redis-stop": "sudo systemctl stop redis",
    "test": "mocha --recursive 'test/**/*.js' --timeout 15000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgesimos/nodejs-mvc-starter.git"
  },
  "keywords": [],
